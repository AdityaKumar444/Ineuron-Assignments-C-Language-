void N_odd_natural(int);

// function to print first N odd natural numbers. (TSRN)

void N_odd_natural(int n)
{
    printf("\nThe first %d odd natural numbers are :\n" , n);
    for (int i = 1 ; i<=2*n ; i++)
    {
        if(i%2!=0)
        printf("%d " , i) ;
    }
    printf("\n") ;
}
