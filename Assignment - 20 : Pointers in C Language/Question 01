#include<stdio.h>
#include<stdlib.h>
void swap(int * , int *);
int main()
{
    system("cls");
    int a , b ;
    printf("Enter two numbers :\n");
    scanf("%d %d", &a , &b);
    swap(&a,&b);
    printf("Swapped values are : %d %d" , a , b);
    return 0 ;
}
void swap(int *p , int *q)
{
    int temp ;
    temp = *p ;
    *p = *q ;
    *q = temp ;
}